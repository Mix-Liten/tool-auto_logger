{
  "name": "logger",
  "version": "1.0.0",
  "description": "Auto logger",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build_win": "pkg index.js --public --targets node14-win-x64 --output dist/logger",
    "build_mac": "pkg index.js --public --targets node14-macos-x64 --output dist/logger",
    "postbuild": "cp -R node_modules/puppeteer/.local-chromium dist/.local-chromium"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.0",
    "dotenv": "^14.3.2",
    "google-spreadsheet": "^3.2.0",
    "node-cron": "^3.0.0",
    "puppeteer": "^13.3.2"
  },
  "devDependencies": {
    "pkg": "^5.5.2"
  },
  "pkg": {
    "scripts": "node_modules/puppeteer/lib/*.js"
  }
}
